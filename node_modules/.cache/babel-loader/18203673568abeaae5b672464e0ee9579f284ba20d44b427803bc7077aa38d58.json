n    created() {\n        API.post('/register', this.new_user)\n            .then(response => {\n                console.log('Post API has received data')\n                console.log(response.data)\n            })\n            .catch(err => {\n                console.log(err)\n            })\n    },\n    methods: {\n        // Example starter JavaScript for disabling form submissions if there are invalid fields\n        function () {\n            'use strict'\n\n            // Fetch all the forms we want to apply custom Bootstrap validation styles to\n            var forms = document.querySelectorAll('.needs-validation')\n\n            // Loop over them and prevent submission\n            Array.prototype.slice.call(forms)\n                .forEach(function (form) {\n                    form.addEventListener('submit', function (event) {\n                        if (!form.checkValidity()) {\n                            event.preventDefault()\n                            event.stopPropagation()\n                        }\n\n                        form.classList.add('was-validated')\n                    }, false)\n                })\n        }\n    }\n}\n</script>\n\n<style scoped></style>"],"mappings":"AA+CA,SAASA,GAAE,QAAS,cAAa;AACjC,OAAOC,MAAK,MAAO,0BAAyB;AAC5C;AACA;;AAEA,eAAe;EACXC,IAAI,EAAE,UAAU;EAChB;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACAC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE;QACNC,SAAS,EAAE,EAAE;QACbC,UAAU,EAAE,EAAE;QACdC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,SAAS,EAAE,EAAE;QACbC,SAAS,EAAE;MACf;IACJ;EACJ,CAAC;EACD;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACAC,UAAU,EAAE;IACRV;EACJ,CAAC;EACDW,OAAOA,CAAA,EAAG;IACNZ,GAAG,CAACa,IAAI,CAAC,WAAW,EAAE,IAAI,CAACT,QAAQ,EAC9BU,IAAI,CAACC,QAAO,IAAK;MACdC,OAAO,CAACC,GAAG,CAAC,4BAA4B;MACxCD,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACZ,IAAI;IAC7B,CAAC,EACAe,KAAK,CAACC,GAAE,IAAK;MACVH,OAAO,CAACC,GAAG,CAACE,GAAG;IACnB,CAAC;EACT,CAAC;EACDC,OAAO,EAAE;IACL;IACAC,QAAOA,CAAA,EAAK;MACR,YAAW;;MAEX;MACA,IAAIC,KAAI,GAAIC,QAA