{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { API } from '../axios-api';\nimport Navbar from '../components/Navbar.vue';\nexport default {\n  name: 'Results',\n  components: {\n    Navbar\n  },\n  data() {\n    return {\n      attempts: [],\n      marks: [],\n      loaded: false\n    };\n  },\n  created() {\n    API.get(`/cards/decks/${this.$route.params.slug}`).then(response => {\n      console.log(response.data);\n      let deck = response.data;\n      API.get(`/cards/attempts`).then(response => {\n        console.log(response.data);\n        let attempts = response.data;\n        attempts.forEach(a => {\n          if (a.deck == deck.id) {\n            this.attempts.push(a);\n          }\n        });\n        this.loaded = true;\n      }).catch(err => {\n        console.log(err);\n      });\n    }).catch(err => {\n      console.log(err);\n    });\n    API.get(`/cards/attempts/${this.$route.params.id}/marks`).then(response => {\n      console.log(response.data);\n      this.marks = response.data;\n      this.loaded = true;\n    }).catch(err => {\n      console.log(err);\n    });\n  },\n  computed: {\n    total() {\n      let marks = this.marks;\n      let total_mark = 0;\n      marks.forEach(m => {\n        total_mark += m.mark;\n      });\n      return total_mark;\n    }\n  },\n  methods: {\n    word(back) {\n      let back_words = back.split(/\\r?\\n/);\n      let word = back_words[0];\n      return word;\n    }\n  }\n};","map":{"version":3,"names":["API","Navbar","name","components","data","attempts","marks","loaded","created","get","$route","params","slug","then","response","console","log","deck","forEach","a","id","push","catch","err","computed","total","total_mark","m","mark","methods","word","back","back_words","split"],"sources":["/Users/elinaigdavletova/Documents/Интернет_программирование/GQW/frontend/vueapp/src/card_views/MyTest.vue"],"sourcesContent":["<template>\n    <Navbar></Navbar>\n    <div class=\"container\">\n        