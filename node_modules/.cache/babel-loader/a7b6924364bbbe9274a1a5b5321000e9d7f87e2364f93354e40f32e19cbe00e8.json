)\n    },\n    methods: {\n        handleclick() {\n            var task_url = 'http://127.0.0.1:8000' + this.APIData.audio\n            this.task_pl.src = task_url\n            this.task_pl.play()\n\n            this.task_pl.addEventListener('ended', () => {\n                this.getBar(this.APIData.prepare_time * 10)\n\n                this.timeouts.push(setTimeout(() => {\n                    this.speak_pl.src = this.BaseData.start_speaking\n                    this.speak_pl.play()\n                }, this.APIData.prepare_time * 1000))\n            })\n\n            this.speak_pl.addEventListener('ended', () => {\n                this.getBar(this.APIData.prepare_time * 10)\n\n                this.timeouts.push(setTimeout(() => {\n                    this.end_pl.src = this.BaseData.end\n                    this.end_pl.play()\n                }, this.APIData.record_time * 1000))\n            })\n        },\n        getBar(time) {\n            let bar = document.getElementById('pr_bar')\n            let percent = 0\n            bar.style.width = percent + '%'\n            let timerid = setInterval(function () {\n                percent += 1\n                bar.style.width = percent + '%'\n\n                if (percent >= 100) {\n                    bar.style.width = '100%'\n                    clearInterval(timerid)\n                }\n            }, time)\n        },\n    }\n}\n</script>\n\n<style scoped></style>"],"mappings":";;EAEaA,KAAK,EAAC;AAAQ;;EAEVA,KAAK,EAAC;AAAW;;EACbA,KAAK,EAAC;AAAM;;EACVC,KAA8B,EAA9B;IAAA;EAAA;AAA8B;;;EAEhCD,KAAK,EAAC;;;EACFA,KAAK,EAAC;AAA+B;;;EAGhCA,KAAK,EAAC;;;EAEXA,KAAK,EAAC;AAA+B;;EAGjCA,KAAK,EAAC,mBAAmB;EAACE,EAAE,EAAC,QAAQ;EAACC,GAAG,EAAC,EAAE;EAACC,GAAG,EAAC;;;EAM7DJ,KAAK,EAAC;AAAwF;iCAC/FK,mBAAA,CAIM;EAJDL,KAAK,EAAC;AAAM,I,aACbK,mBAAA,CAEM;EAFDL,KAAK,EAAC;AAAU,I,aACjBK,mBAAA,CAA2J;EAAtJL,KAAK,EAAC,gCAAgC;EAACM,IAAI,EAAC,aAAa;EAAC,eAAa,EAAC,GAAG;EAAC,eAAa,EAAC,GAAG;EAAC,eAAa,EAAC,KAAK;EAACL,KAAgB,EAAhB;IAAA;EAAA,CAAgB;EAACC,EAAE,