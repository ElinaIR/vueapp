{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { API } from '../axios-api';\nimport Navbar from '../components/Navbar.vue';\nexport default {\n  name: 'Results',\n  components: {\n    Navbar\n  },\n  data() {\n    return {\n      attempts: [],\n      marks: [],\n      loaded: false\n    };\n  },\n  created() {\n    API.get(`/cards/decks/${this.$route.params.slug}`).then(response => {\n      console.log(response.data);\n      let deck = response.data;\n      API.get(`/cards/attempts`).then(response => {\n        console.log(response.data);\n        let attempts = response.data;\n        attempts.forEach(a => {\n          if (a.deck == deck.id) {\n            this.attempts.push(a);\n          }\n        });\n        API.get(`/cards/marks`).then(response => {\n          console.log(response.data);\n          let marks = response.data;\n          marks.forEach(m => {\n            if (m.card.deck.id == deck.id) {\n              this.marks.push(m);\n            }\n          });\n          this.loaded = true;\n        }).catch(err => {\n          console.log(err);\n        });\n      }).catch(err => {\n        console.log(err);\n      });\n    }).catch(err => {\n      console.log(err);\n    });\n  },\n  methods: {\n    mark(id) {\n      let total_mark = 0;\n      let marks = this.marks;\n      marks.forEach(m => {\n        if (m.attempt == id) {\n          total_mark += m.mark;\n        }\n      });\n      return total_mark;\n    }\n  }\n};","map":{"version":3,"names":["API","Navbar","name","components","data","attempts","marks","loaded","created","get","$route","params","slug","then","response","console","log","deck","forEach","a","id","push","m","card","catch","err","methods","mark","total_mark","attempt"],"sources":["/Users/elinaigdavletova/Documents/Интернет_программирование/GQW/frontend/vueapp/src/card_views/MyTest.vue"],"sourcesContent":["<template>\n    <Navbar></Navbar>\n    <div class=\"container\">\n        <table class=\"table table-light my-4 table-bordered\" v-if=\"loaded\">\n            <thead>\n                <tr>\n                    <th>Пользователь</th>\n                    <th>Результат</th>\n                    <th>Дата</th>\n                </tr>\n            </thead>\n            <tbody>\n                <tr v-for=\"a in attempts\" :key=\"a.id\">\n                    <td>\n                        <span v-if=\"a.user.last_name || a.user.first_name\">\n                            {{ a.user.last_name }} {{ a.user.first_name }}\n                        </span>\n                        <span v-else>\n                            {{ a.user.username }}\n                        </span>\n                    </td>\n                    <td>{{ mark(a.id) }}</td>\n                    <td>{{ a.date }}</td>\n                </tr>\n            </tbody>\n        </table>\n    </div>\n</template>\n\n<script>\nimport { API } from '../axios-api'\nimport Navbar from '../components/Navbar.vue'\nexport default {\n    name: 'Results',\n    components: {\n        Navbar\n    },\n    data() {\n        return {\n            attempts: [],\n            marks: [],\n            loaded: false,\n        }\n    },\n    created() {\n        API.get(`/cards/decks/${this.$route.params.slug}`).then(response => {\n            console.log(response.data)\n            let deck = response.data\n            API.get(`/cards/attempts`).then(response => {\n                console.log(response.data)\n                let attempts = response.data\n                attempts.forEach(a => {\n                    if (a.deck == deck.id) {\n                        this.attempts.push(a)\n                    }\n                })\n                \n                API.get(`/cards/marks`).then(response => {\n                    console.log(response.data)\n                    let marks = response.data\n                    marks.forEach(m => {\n                        if (m.card.deck.id == deck.id) {\n                            this.marks.push(m)\n                        }\n                    })\n                    this.loaded = true\n                }).catch(err => {\n                    console.log(err)\n                })\n            }).catch(err => {\n                console.log(err)\n            })\n        }).catch(err => {\n            console.log(err)\n        })\n    },\n    methods: {\n        mark(id) {\n            let total_mark = 0\n            let marks = this.marks\n            marks.forEach(m => {\n                if (m.attempt == id) {\n                    total_mark += m.mark\n                }\n            })\n\n            return total_mark\n        }\n    }\n}\n</script>\n\n<style scoped></style>"],"mappings":";AA8BA,SAASA,GAAE,QAAS,cAAa;AACjC,OAAOC,MAAK,MAAO,0BAAyB;AAC5C,eAAe;EACXC,IAAI,EAAE,SAAS;EACfC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE;IACZ;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACNR,GAAG,CAACS,GAAG,CAAE,gBAAe,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,IAAK,EAAC,CAAC,CAACC,IAAI,CAACC,QAAO,IAAK;MAChEC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACV,IAAI;MACzB,IAAIa,IAAG,GAAIH,QAAQ,CAACV,IAAG;MACvBJ,GAAG,CAACS,GAAG,CAAE,iBAAgB,CAAC,CAACI,IAAI,CAACC,QAAO,IAAK;QACxCC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACV,IAAI;QACzB,IAAIC,QAAO,GAAIS,QAAQ,CAACV,IAAG;QAC3BC,QAAQ,CAACa,OAAO,CAACC,CAAA,IAAK;UAClB,IAAIA,CAAC,CAACF,IAAG,IAAKA,IAAI,CAACG,EAAE,EAAE;YACnB,IAAI,CAACf,QAAQ,CAACgB,IAAI,CAACF,CAAC;UACxB;QACJ,CAAC;QAEDnB,GAAG,CAACS,GAAG,CAAE,cAAa,CAAC,CAACI,IAAI,CAACC,QAAO,IAAK;UACrCC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACV,IAAI;UACzB,IAAIE,KAAI,GAAIQ,QAAQ,CAACV,IAAG;UACxBE,KAAK,CAACY,OAAO,CAACI,CAAA,IAAK;YACf,IAAIA,CAAC,CAACC,IAAI,CAACN,IAAI,CAACG,EAAC,IAAKH,IAAI,CAACG,EAAE,EAAE;cAC3B,IAAI,CAACd,KAAK,CAACe,IAAI,CAACC,CAAC;YACrB;UACJ,CAAC;UACD,IAAI,CAACf,MAAK,GAAI,IAAG;QACrB,CAAC,CAAC,CAACiB,KAAK,CAACC,GAAE,IAAK;UACZV,OAAO,CAACC,GAAG,CAACS,GAAG;QACnB,CAAC;MACL,CAAC,CAAC,CAACD,KAAK,CAACC,GAAE,IAAK;QACZV,OAAO,CAACC,GAAG,CAACS,GAAG;MACnB,CAAC;IACL,CAAC,CAAC,CAACD,KA