uuidv4() + '.png';\n      fd.append('pic2', this.task4pic2, fileName);\n      API.post('/ege/task4/', fd, {\n        headers: {\n          'Content-Type': 'multipart/form-data',\n          'X-CSRFToken': localStorage.getItem('csrfToken')\n        }\n      }).then(response => {\n        console.log('task4', response.data, response.data.id);\n        // this.tasks.task4 = response.data.id\n        id4 = response.data.id;\n        // console.log(this.tasks.task4)\n      }).catch(err => {\n        console.log(err);\n      });\n\n      // console.log(this.tasks)\n      // var all_tasks = true\n      // var missing = []\n      // for (let i in this.tasks) {\n      //     console.log(i.valueOf())\n      //     if (i.valueOf() == null) {\n      //         all_tasks = false\n      //         missing.push(i)\n      //     }\n      // }\n      // if (all_tasks) {\n\n      // fd = new FormData()\n      // fd.append('name', this.name)\n      // fd.append('task1', this.tasks.task1)\n      // fd.append('task2', this.tasks.task2)\n      // fd.append('task3', this.tasks.task3)\n      // fd.append('task4', this.tasks.task4)\n      console.log(this.tasks);\n      API.post('/ege/variants/', {\n        name: this.name,\n        task1: id1,\n        task2: id2,\n        task3: id3,\n        task4: id4\n      }, {\n        headers: {\n          'Authorization': `Bearer ${this.$store.state.accessToken}`,\n          // 'Content-Type': 'multipart/form-data',\n          'X-CSRFToken': localStorage.getItem('csrfToken')\n        }\n        // data: {\n        //     name: this.name,\n        //     task1: this.tasks.task1,\n        //     task2: this.tasks.task2,\n        //     task3: this.tasks.task3,\n        //     task4: this.tasks.task4,\n        // }\n      }).then(response => {\n        console.log('variant created', response.data);\n      }).catch(err => {\n        console.log(err);\n      });\n      // } else {\n      //     for (let i in missing) {\n      //         console.log('Missing task', missing[i])\n      //     }\n      // }\n      // console.log('yes')\n    }\n  }\n};","map":{"version":3,"names":["API","Navbar","v4","uuidv4","name","data","task1","text","task1audio","task2","task2audio","picCap","task2pic","task2q1","task2q2","task2q3","task2q4","task3","task3audio","task3start","task3q1","task3q2","task3q3","task3q4","task3q5","task3end","task4","task4audio","task4pic1","task4pic2","tasks","components","methods","handleTask1audio","e","target","files","handleTask2photo","img","document","getElementById","src","URL","createObjectURL","handleTask2audio","handleTask2q1","handleTask2q2","handleTask2q3","handleTask2q4","handleTask3audio","handleTask3start","handleTask3q1","handleTask3q2","handleTask3q3","handleTask3q4","handleTask3q5","handleTask3end","handleTask4audio","handleTask4photos","img1","img2","add","console","log","fileName","fd","FormData","id1","append","post","headers","localStorage","getItem","then","response","id","catch","err","id2","id3","id4","$store","state","accessToken"],"sources":["/Users/elinaigdavletova/Documents/Интернет_программирование/GQW/frontend/vueapp/src/views/addEGEVariant.vue"],"sourcesContent":["<template>\n    <Navbar></Navbar>\n    <div class=\"container p-5\">\n        <form class=\"p-3 bg-light border border-3 rounded-3\" @submit.prevent=\"add()\">\n            <div class=\"mb-3\">\n                <label for=\"InputName\" class=\"form-label\">Название варианта</label>\n                <input type=\"text\" class=\"form-control\" id=\"InputName\" v-model=\"name\">\n            </div>\n            <div class=\"border-top border-3\">\n                <h5 class=\"mt-2\">Задание 1</h5>\n                <div class=\"row g-3\">\n                    <div class=\"mb-3 col\">\n                        <label for=\"InputTask1Task\" class=\"form-label\">Задание</label>\n                        <textarea rows=\"10\" class=\"form-control\" id=\"InputTask1Task\" v-model=\"task1\"></textarea>\n                    </div>\n                    <div class=\"mb-3 col-9\">\n                        <label for=\"InputText\" class=\"form-label\">Текст</label>\n                        <textarea rows=\"10\" class=\"form-control\" id=\"InputText\" v-model=\"text\"></textarea>\n                    </div>\n                </div>\n                <div class=\"mb-3\">\n                    <label for=\"InputTask1audio\" class=\"form-label\">Загрузите аудио</label>\n                    <input class=\"form-control form-control-sm\" id=\"InputTask1audio\" type=\"file\" @change=\"handleTask1audio\">\n                </div>\n            </div>\n            <div class=\"border-top border-3 row\">\n                <h5 class=\"mt-2\">Задание 2</h5>\n                <div class=\"mb-3 col\">\n                    <label for=\"InputTask2pic\" class=\"form-label\">Загрузите фото</label>\n                    <input class=\"form-control form-control-sm\" id=\"InputTask2pic\" type=\"file\" @change=\"handleTask2photo\">\n                    <img id=\"photo1\" src=\"\" class=\"img-fluid\">\n                </div>\n                <div class=\"col-8\">\n                    <div class=\"mb-3\">\n                        <label for=\"InputName\" class=\"form-label\">Подпись к фото</label>\n                        <input type=\"text\" class=\"form-control\" id=\"InputName\" v-model=\"picCap\">\n                    </div>\n                    <div class=\"mb-3\">\n                        <label for=\"InputTask2Task\" class=\"form-label\">Задание</label>\n                        <textarea rows=\"10\" class=\"form-control\" id=\"InputTask2Task\" v-model=\"task2\"></textarea>\n                    </div>\n                    <div class=\"mb-3\">\n                        <label for=\"InputTask2audio\" class=\"form-label\">Загрузите аудио задания</label>\n                        <input class=\"form-control form-control-sm\" id=\"InputTask2audio\" type=\"file\" @change=\"handleTask2audio\">\n                    </div>\n     