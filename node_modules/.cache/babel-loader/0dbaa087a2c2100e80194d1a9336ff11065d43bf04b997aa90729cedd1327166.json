n                headers: {\n                    'Content-Type': 'multipart/form-data',\n                    'X-CSRFToken': localStorage.getItem('csrfToken')\n                },\n            }).then(response => {\n                console.log('task2', response.data)\n                this.tasks.task2 = response.data.id\n                console.log(this.tasks.task2)\n            }).catch(err => {\n                console.log(err)\n            })\n            API.post('/task3/', {\n                task: this.task3,\n            }).then(response => {\n                console.log('task3', response.data)\n                this.tasks.task3 = response.data.id\n            }).catch(err => {\n                console.log(err)\n            })\n            API.post('/task4/', {\n                task: this.task4,\n            }).then(response => {\n                console.log('task4', response.data)\n                this.tasks.task4 = response.data.id\n            }).catch(err => {\n                console.log(err)\n            })\n            console.log(this.tasks)\n            var all_tasks = true\n            var missing = []\n            for (let i in this.tasks) {\n                console.log(i.valueOf())\n                if (i.valueOf() == null) {\n                    all_tasks = false\n                    missing.push(i)\n                }\n            }\n            if (all_tasks) {\n                API.post('/variants/', {\n                    headers: {\n                        Authorization: `Bearer ${this.$store.state.accessToken}`\n                    },\n                    data: {\n                        name: this.name,\n                        task1: this.tasks.task1,\n                        task2: this.tasks.task2,\n                        task3: this.tasks.task3,\n                        task4: this.tasks.task4,\n                    }\n                }).then(response => {\n                    console.log('variant created', response.data)\n                }).catch(err => {\n                    console.log(err)\n                })\n            } else {\n                for (let i in missing) {\n                    console.log('Missing task', missing[i])\n                }\n            }\n            console.log('yes')\n        },\n    }\n}\n</script>\n\n<style scoped></style>"],"mappings":";AAgEA,SAASA,GAAE,QAAS,cAAa;AACjC,OAAOC,MAAK,MAAO,0BAAyB;AAC5C,SAASC,EAAC,IAAKC,MAAK,QAAS,MAAK;AAElC,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHD,IAAI,EAAE,EAAE;MACRE,KAAK,EAAE,EAAE;MACTC,IAAI,EAAE,EAAE;MACRC,UAAU,EAAE,IAAI;MAChBC,KAAK,EAAE,EAAE;MACTC,UAAU,EAAE,IAAI;MAChBC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,IAAI;MACdC,OAAO,EAAE,IAAI;MACbC,OAAO,EAAE,IAAI;MACbC,OAAO,EAAE,IAAI;MACbC,OAAO,EAAE,IAAI;MACbC,KAAK,EAAE,EAAE;MACTC,UAAU,EAAE,IAAI;MAChBC,KAAK,EAAE,EAAE;MACTC,UAAU,EAAE,IAAI;MAChBC,KAAK,EAAE;QACHf,KAAK,EAAE,IAAI;QACXG,KAAK,EAAE,IAAI;QACXQ,KAAK,EAAE,IAAI;QACXE,KAAK,EAAE;MACX;IACJ;EACJ,CAAC;EACDG,UAAU,EAAE;IACRrB;EACJ,CAAC;EACDsB,OAAO,EAAE;IACLC,gBAAgBA,CAACC,CAAC,EAAE;MAChB,IAAI,CAACb,QAAO,GAAIa,CAAC,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;MACjC,IAAIC,GAAE,GAAIC,QAAQ,CAACC,cAAc,CAAC,OAAO;MACzCF,GAAG,CAACG,GAAE,GAAIC,GAAG,CAACC,eAAe,CAACR,CAAC,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;IACnD,CAAC;IACDO,GAAGA,CAAA,EAAG;MACFC,OAAO,CAACC,GAAG,CAAC,MAAM,CAAC;MACnB,IAAIC,QAAO,GAAIlC,MAAM,EAAC,GAAI,MAAM;MAChC,MAAMmC,QAAO,GAAI,IAAIC,QAAQ,EAAE;MAC/BJ,OAAO,CAACC,GAAG,CAAC,IAAI,CAACxB,QAAQ;MACzB;MACA0B,QAAQ,CAACE,MAAM,CAAC,KAAK,EAAE,IAAI,CAAC5B,QAAQ,EAAEyB,QAAQ;MAC9CF,OAAO,CAACC,GAAG,CAACC,QAAQ;MACpBC,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAAC/B,KAAK;MAClC6B,QAAQ,CAACE,MAAM,CAAC,aAAa,EAAE,IAAI,CAAC7B,MAAM;MAC1CwB,OAAO,CAACC,GAAG,CAACE,QAAQ;MACpBH,OAAO,CAACC,GAAG,CAAC,6BAA6B;MACzC;MACApC,GAAG,CAACyC,IAAI,CAAC,SAAS,EAAE;QAChBC,IAAI,EAAE,IAAI,CAACpC,KAAK;QAChBC,IAAI,EAAE,IAAI,CAACA;MACf,CAAC,CAAC,CAACoC,IAAI,CAACC,QAAO,IAAK;QAChBT,OAAO,CAACC,GAAG,CAAC,OAAO,EAAEQ,QAAQ,CAACvC,IAAI;QAClC,IAAI,CAACgB,KAAK,CAACf,KAAI,GAAIsC,QAAQ,CAACvC,IAAI,CAACwC,