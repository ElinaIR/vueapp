{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { API } from '../axios-api';\nimport Navbar from '../components/Navbar.vue';\nexport default {\n  name: 'Task1',\n  // props: ['id'],\n  data() {\n    return {\n      counteddown: false,\n      countdown: 3,\n      start: false,\n      APIData: '',\n      BaseData: '',\n      start_pl: new Audio(),\n      task_pl: new Audio(),\n      speak_pl: new Audio(),\n      startTimeout: '',\n      prepareTimeout: '',\n      recordTimeout: '',\n      timeouts: []\n    };\n  },\n  components: {\n    Navbar\n  },\n  computed: {\n    ege: function () {\n      return this.$route.params.exam == 'ege';\n    },\n    oge: function () {\n      return this.$route.params.exam == 'oge';\n    },\n    vpr: function () {\n      return this.$route.params.exam == 'vpr';\n    }\n  },\n  created() {\n    API.get(`/${this.$route.params.exam}/variants/${this.$route.params.id}/tasks`).then(response => {\n      console.log(response.data);\n      this.APIData = response.data.tasks[0];\n    }).catch(err => {\n      console.log(err);\n    });\n    API.get(`/base-audio`).then(response => {\n      console.log(response.data);\n      this.BaseData = response.data.at(-1);\n    }).catch(err => {\n      console.log(err);\n    });\n  },\n  mounted() {\n    let start_interval = setInterval(() => {\n      this.countdown--;\n    }, 1000);\n    setTimeout(() => {\n      clearInterval(start_interval);\n      this.counteddown = true;\n      this.handleclick();\n    }, 3000);\n  },\n  beforeRouteLeave() {\n    for (let i = 0; i < this.timeouts.length; i++) {\n      clearTimeout(this.timeouts[i]);\n    }\n    this.start_pl.pause();\n    this.task_pl.pause();\n    this.speak_pl.pause();\n  },\n  methods: {\n    handleclick() {\n      this.start_pl.src = this.BaseData.start;\n      this.start_pl.play();\n      this.start_pl.addEventListener('ended', () => {\n        console.log(this.start, 'thisstatr');\n        this.start = true;\n        console.log(this.start);\n        var task_url = 'http://127.0.0.1:8000' + this.APIData.audio;\n        this.task_pl.src = task_url;\n        console.log(this.task_pl);\n        this.task_pl.play();\n      });\n      this.task_pl.addEventListener('ended', () => {\n        var bar = document.getElementById('pr_bar');\n        var percent = 0;\n        bar.style.width = percent + '%';\n        var timerid = setInterval(function () {\n          percent += 1;\n          bar.style.width = percent + '%';\n          if (percent >= 100) {\n            bar.style.width = '100%';\n            clearInterval(timerid);\n          }\n        }, this.APIData.prepare_time * 10);\n        this.timeouts.push(setTimeout(() => {\n          this.speak_pl.src = this.BaseData.start_speaking;\n          this.speak_pl.play();\n        }, this.APIData.prepare_time * 1000));\n      });\n      this.speak_pl.addEventListener('ended', () => {\n        var bar = document.getElementById('pr_bar');\n        var percent = 0;\n        bar.style.width = percent + '%';\n        var timerid = setInterval(function () {\n          percent += 1;\n          bar.style.width = percent + '%';\n          if (percent >= 100) {\n            bar.style.width = '100%';\n            clearInterval(timerid);\n          }\n        }, this.APIData.record_time * 10);\n        this.timeouts.push(setTimeout(() => {\n          if (this.ege) {\n            this.$router.push({\n              name: 'ege-task2',\n              params: {\n                exam: this.$route.params.exam,\n                id: this.$route.params.id\n              }\n            });\n          } else if (this.oge) {\n            this.$router.push({\n              name: 'ege-task3',\n              params: {\n                exam: this.$route.params.exam,\n                id: this.$route.params.id\n              }\n            });\n          } else if (this.vpr) {\n            this.$router.push({\n              name: 'ege-task4',\n              params: {\n                exam: this.$route.params.exam,\n                id: this.$route.params.id\n              }\n            });\n          }\n        }, this.APIData.record_time * 1000));\n      });\n    }\n  }\n};","map":{"version":3,"names":["API","Navbar","name","data","counteddown","countdown","start","APIData","BaseData","start_pl","Audio","task_pl","speak_pl","startTimeout","prepareTimeout","recordTimeout","timeouts","components","computed","ege","$route","params","exam","oge","vpr","created","get","id","then","response","console","log","tasks","catch","err","at","mounted","start_interval","setInterval","setTimeout","clearInterval","handleclick","beforeRouteLeave","i","length","clearTimeout","pause","methods","src","play","addEventListener","task_url","audio","bar","document","getElementById","percent","style","width","timerid","prepare_time","push","start_speaking","record_time","$router"],"sources":["/Users/elinaigdavletova/Documents/Интернет_программирование/GQW/frontend/vueapp/src/views/Task1.vue"],"sourcesContent":["<template>\n    <div tabindex=\"-1\" @keypress=\"handleclick\">\n    <div class=\"vh-100 position-relative\" :class=\"{'d-flex': !start, 'flex-column': !start, 'justify-content-center': !start }\">\n        <div :class=\"{'fixed-top': !start}\"><Navbar></Navbar></div>\n        <div class=\"container\">\n            <h1 v-if=\"!counteddown\" class=\"text-center text-danger\"><strong>{{ countdown }}</strong></h1>\n            <h1 v-if=\"counteddown && !start\" class=\"text-center text-danger\"><strong>Now we are ready to start</strong></h1>\n\n\n            <div v-if=\"start\" class=\"mt-2\">\n                 <p><strong>{{ APIData.task }}</strong></p>\n                 <p>{{ APIData.text }}</p>\n            </div>\n            <!-- <div class=\"position-absolute bottom-0 w-75 m-5\">\n                <div class=\"progress\">\n                    <div class=\"progress-bar progress-bar-info\" role=\"progressbar\" aria-valuenow=\"0\" aria-valuemin=\"0\" aria-valuemax=\"100\" style=\"width:0%\" id=\"pr_bar\" ></div>\n                </div>\n            </div> -->\n        </div>\n        <div class=\"position-absolute bottom-0 w-100 d-flex justify-content-around align-items-center mb-3\" v-show=\"start\">\n            <div class=\"w-75\">\n                <div class=\"progress\">\n                    <div class=\"progress-bar progress-bar-info\" role=\"progressbar\" aria-valuenow=\"0\" aria-valuemin=\"0\" aria-valuemax=\"100\" style=\"width:0%\" id=\"pr_bar\" ></div>\n                </div>\n            </div>\n            <div>\n                <!-- <button class=\"btn btn-outline-secondary \" v-if=\"start\" @click=\"handleSkip\">Пропустить</button> -->\n                <router-link :to=\"{ name: 'ege-task2', params: { exam: this.$route.params.exam, id: this.$route.params.id } }\" class=\"btn btn-outline-secondary\" v-if=\"ege\">Пропустить</router-link>\n                <router-link :to=\"{ name: 'ege-task3', params: { exam: this.$route.params.exam, id: this.$route.params.id } }\" class=\"btn btn-outline-secondary\" v-else-if=\"oge\">Пропустить</router-link>\n                <router-link :to=\"{ name: 'ege-task4', params: { exam: this.$route.params.exam, id: this.$route.params.id } }\" class=\"btn btn-outline-secondary\" v-else-if=\"vpr\">Пропустить</router-link>\n            </div>\n        </div>\n    </div>\n    </div>\n</template>\n\n<script>\nimport { API } from '../axios-api'\nimport Navbar from '../components/Navbar.vue'\nexport default {\n    name: 'Task1',\n    // props: ['id'],\n    data() {\n        return {\n            counteddown: false,\n            countdown: 3,\n            start: false,\n            APIData: '',\n            BaseData: '',\n            start_pl: new Audio(),\n            task_pl: new Audio(),\n            speak_pl: new Audio(),\n            startTimeout: '',\n            prepareTimeout: '',\n            recordTimeout: '',\n            timeouts: [],\n        }\n    },\n    components: {\n        Navbar\n    },\n    computed: {\n        ege: function() {\n            return this.$route.params.exam == 'ege'\n        },\n        oge: function () {\n            return this.$route.params.exam == 'oge'\n        },\n        vpr: function () {\n            return this.$route.params.exam == 'vpr'\n        },\n    },\n    created() {\n        API.get(`/${this.$route.params.exam}/variants/${this.$route.params.id}/tasks`,)\n            .then(response => {\n                console.log(response.data)\n                this.APIData = response.data.tasks[0]\n            })\n            .catch(err => {\n                console.log(err)\n            })\n        API.get(`/base-audio`,)\n            .then(response => {\n                console.log(response.data)\n                this.BaseData = response.data.at(-1)\n            })\n            .catch(err => {\n                console.log(err)\n            })\n    },\n    mounted() {\n        let start_interval = setInterval(() => {\n            this.countdown--\n        }, 1000)\n\n        setTimeout(() => {\n            clearInterval(start_interval)\n            this.counteddown = true\n            this.handleclick()\n        }, 3000)\n\n    },\n    beforeRouteLeave() {\n        for (let i = 0; i < this.timeouts.length; i++) {\n            clearTimeout(this.timeouts[i])\n        }\n        this.start_pl.pause()\n        this.task_pl.pause()\n        this.speak_pl.pause()\n    },\n    methods: {\n        handleclick() {\n            this.start_pl.src = this.BaseData.start\n            this.start_pl.play()\n\n            this.start_pl.addEventListener('ended',  () => {\n                console.log(this.start, 'thisstatr')\n                this.start = true\n                console.log(this.start)\n                var task_url = 'http://127.0.0.1:8000' + this.APIData.audio\n                this.task_pl.src = task_url\n                console.log(this.task_pl)\n                this.task_pl.play()\n            })\n\n            this.task_pl.addEventListener('ended', () => {\n                var bar = document.getElementById('pr_bar')\n                var percent = 0\n                bar.style.width = percent + '%'\n                var timerid = setInterval(function () {\n                    percent += 1\n                    bar.style.width = percent + '%'\n\n                    if (percent >= 100) {\n                        bar.style.width = '100%'\n                        clearInterval(timerid)\n                    }\n                }, this.APIData.prepare_time * 10)\n\n                this.timeouts.push(setTimeout(() => {\n                    this.speak_pl.src = this.BaseData.start_speaking\n                    this.speak_pl.play()\n                }, this.APIData.prepare_time * 1000))\n            })\n\n            this.speak_pl.addEventListener('ended', () => {\n                var bar = document.getElementById('pr_bar')\n                var percent = 0\n                bar.style.width = percent + '%'\n                var timerid = setInterval(function () {\n                    percent += 1\n                    bar.style.width = percent + '%'\n\n                    if (percent >= 100) {\n                        bar.style.width = '100%'\n                        clearInterval(timerid)\n                    }\n                }, this.APIData.record_time * 10)\n\n                this.timeouts.push(setTimeout(() => {\n                    if (this.ege) {\n                        this.$router.push({ name: 'ege-task2', params: { exam: this.$route.params.exam, id: this.$route.params.id } })\n                    }\n                    else if (this.oge) {\n                        this.$router.push({ name: 'ege-task3', params: { exam: this.$route.params.exam, id: this.$route.params.id } })\n                    }\n                    else if (this.vpr) {\n                        this.$router.push({ name: 'ege-task4', params: { exam: this.$route.params.exam, id: this.$route.params.id } })\n                    }\n                }, this.APIData.record_time * 1000))\n            })\n        },\n    }\n}\n</script>\n\n<style scoped></style>"],"mappings":";AAqCA,SAASA,GAAE,QAAS,cAAa;AACjC,OAAOC,MAAK,MAAO,0BAAyB;AAC5C,eAAe;EACXC,IAAI,EAAE,OAAO;EACb;EACAC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,WAAW,EAAE,KAAK;MAClBC,SAAS,EAAE,CAAC;MACZC,KAAK,EAAE,KAAK;MACZC,OAAO,EAAE,EAAE;MACXC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,IAAIC,KAAK,EAAE;MACrBC,OAAO,EAAE,IAAID,KAAK,EAAE;MACpBE,QAAQ,EAAE,IAAIF,KAAK,EAAE;MACrBG,YAAY,EAAE,EAAE;MAChBC,cAAc,EAAE,EAAE;MAClBC,aAAa,EAAE,EAAE;MACjBC,QAAQ,EAAE;IACd;EACJ,CAAC;EACDC,UAAU,EAAE;IACRhB;EACJ,CAAC;EACDiB,QAAQ,EAAE;IACNC,GAAG,EAAE,SAAAA,CAAA,EAAW;MACZ,OAAO,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,IAAG,IAAK,KAAI;IAC1C,CAAC;IACDC,GAAG,EAAE,SAAAA,CAAA,EAAY;MACb,OAAO,IAAI,CAACH,MAAM,CAACC,MAAM,CAACC,IAAG,IAAK,KAAI;IAC1C,CAAC;IACDE,GAAG,EAAE,SAAAA,CAAA,EAAY;MACb,OAAO,IAAI,CAACJ,MAAM,CAACC,MAAM,CAACC,IAAG,IAAK,KAAI;IAC1C;EACJ,CAAC;EACDG,OAAOA,CAAA,EAAG;IACNzB,GAAG,CAAC0B,GAAG,CAAE,IAAG,IAAI,CAACN,MAAM,CAACC,MAAM,CAACC,IAAK,aAAY,IAAI,CAACF,MAAM,CAACC,MAAM,CAACM,EAAG,QAAO,CAAC,CACzEC,IAAI,CAACC,QAAO,IAAK;MACdC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC1B,IAAI;MACzB,IAAI,CAACI,OAAM,GAAIsB,QAAQ,CAAC1B,IAAI,CAAC6B,KAAK,CAAC,CAAC;IACxC,CAAC,EACAC,KAAK,CAACC,GAAE,IAAK;MACVJ,OAAO,CAACC,GAAG,CAACG,GAAG;IACnB,CAAC;IACLlC,GAAG,CAAC0B,GAAG,CAAE,aAAY,CAAC,CACjBE,IAAI,CAACC,QAAO,IAAK;MACdC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC1B,IAAI;MACzB,IAAI,CAACK,QAAO,GAAIqB,QAAQ,CAAC1B,IAAI,CAACgC,EAAE,CAAC,CAAC,CAAC;IACvC,CAAC,EACAF,KAAK,CAACC,GAAE,IAAK;MACVJ,OAAO,CAACC,GAAG,CAACG,GAAG;IACnB,CAAC;EACT,CAAC;EACDE,OAAOA,CAAA,EAAG;IACN,IAAIC,cAAa,GAAIC,WAAW,CAAC,MAAM;MACnC,IAAI,CAACjC,SAAS,EAAC;IACnB,CAAC,EAAE,IAAI;IAEPkC,UAAU,CAAC,MAAM;MACbC,aAAa,CAACH,cAAc;MAC5B,IAAI,CAACjC,WAAU,GAAI,IAAG;MACtB,IAAI,CAACqC,WAAW,EAAC;IACrB,CAAC,EAAE,IAAI;EAEX,CAAC;EACDC,gBAAgBA,CAAA,EAAG;IACf,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAI,IAAI,CAAC3B,QAAQ,CAAC4B,MAAM,EAAED,CAAC,EAAE,EAAE;MAC3CE,YAAY,CAAC,IAAI,CAAC7B,QAAQ,CAAC2