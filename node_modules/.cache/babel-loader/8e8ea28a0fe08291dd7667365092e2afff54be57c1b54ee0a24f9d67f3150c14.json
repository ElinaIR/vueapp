              })\n                console.log(total_mark)\n                return `${total_mark}`\n            }).catch(err => {\n                console.log(err)\n            })\n        }\n    }\n}\n</script>\n\n<style scoped></style>"],"mappings":";AA8BA,SAASA,GAAE,QAAS,cAAa;AACjC,OAAOC,MAAK,MAAO,0BAAyB;AAC5C,eAAe;EACXC,IAAI,EAAE,SAAS;EACfC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE;IACZ;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACNR,GAAG,CAACS,GAAG,CAAE,gBAAe,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,IAAK,EAAC,CAAC,CAACC,IAAI,CAACC,QAAO,IAAK;MAChEC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACV,IAAI;MACzB,IAAIa,IAAG,GAAIH,QAAQ,CAACV,IAAG;MACvBJ,GAAG,CAACS,GAAG,CAAE,iBAAgB,CAAC,CAACI,IAAI,CAACC,QAAO,IAAK;QACxCC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACV,IAAI;QACzB,IAAIC,QAAO,GAAIS,QAAQ,CAACV,IAAG;QAC3BC,QAAQ,CAACa,OAAO,CAACC,CAAA,IAAK;UAClB,IAAIA,CAAC,CAACF,IAAG,IAAKA,IAAI,CAACG,EAAE,EAAE;YACnB,IAAI,CAACf,QAAQ,CAACgB,IAAI,CAACF,CAAC;UACxB;QACJ,CAAC;QACD,IAAI,CAACZ,MAAK,GAAI,IAAG;MACrB,CAAC,CAAC,CAACe,KAAK,CAACC,GAAE,IAAK;QACZR,OAAO,CAACC,GAAG,CAACO,GAAG;MACnB,CAAC;IACL,CAAC,CAAC,CAACD,KAAK,CAACC,GAAE,IAAK;MACZR,OAAO,CAACC,GAAG,CAACO,GAAG;IACnB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,IAAIA,CAACL,EAAE,EAAE;MACLpB,GAAG,CAACS,GAAG,CAAE,mBAAkBW,EAAG,QAAO,CAAC,CAACP,IAAI,CAACC,QAAO,IAAK;QACpDC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACV,IAAI;QACzB,IAAIE,KAAI,GAAIQ,QAAQ,CAACV,IAAG;QACxB,IAAIsB,UAAS,GAAI;QACjBpB,KAAK,CAACY,OAAO,CAACS,CAAA,IAAK;UACfD,UAAS,IAAKC,CAAC,CAACF,IAAG;QACvB,CAAC;QACDV,OAAO,CAACC,GAAG,CAACU,UAAU;QACtB,OAAQ,GAAEA,UAAW;MACzB,CAAC,CAAC,CAACJ,KAAK,CAACC,GAAE,IAAK;QACZR,OAAO,CAACC,GAAG,CAACO,GAAG;MACnB,CAAC;IACL;EACJ;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}