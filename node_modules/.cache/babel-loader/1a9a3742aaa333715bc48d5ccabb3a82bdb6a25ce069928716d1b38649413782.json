   this.end_pl.src = this.BaseData.end\n                    this.end_pl.play()\n                }, this.APIData.record_time * 1000))\n            })\n\n            this.end_pl.addEventListener('ended', () => {\n                if (user) {\n                    if (this.$route.params.exam == 'ege') {\n                        API.post('/popularity/', {\n                            variant_type: this.$route.params.exam,\n                            ege_variant: this.$route.params.id,\n                            user: this.user.id\n                        }).then(response => {\n                            console.log(response)\n                            this.$router.push({ name: 'exam', params: { exam: this.$route.params.exam } })\n                        }).catch(err => {\n                            console.log(err)\n                        })\n                    }\n                    else if (this.$route.params.exam == 'oge') {\n                        API.post('/popularity/', {\n                            variant_type: this.$route.params.exam,\n                            oge_variant: this.$route.params.id,\n                            user: this.user.id\n                        }).then(response => {\n                            console.log(response)\n                            this.$router.push({ name: 'exam', params: { exam: this.$route.params.exam } })\n                        }).catch(err => {\n                            console.log(err)\n                        })\n                    }\n                    else if (this.$route.params.exam == 'vpr') {\n                        API.post('/popularity/', {\n                            variant_type: this.$route.params.exam,\n                            vpr_variant: this.$route.params.id,\n                            user: this.user.id\n                        }).then(response => {\n                            console.log(response)\n                            this.$router.push({ name: 'exam', params: { exam: 